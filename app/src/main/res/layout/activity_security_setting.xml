<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".ui.activitys.SecuritySettingActivity">
    <com.zhy.autolayout.AutoRelativeLayout
        android:background="#34393f"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <include layout="@layout/toolbar"/>
    </com.zhy.autolayout.AutoRelativeLayout>
    <com.zhy.autolayout.AutoRelativeLayout
        android:id="@+id/zhanghaoanquan"
        android:layout_marginTop="13px"
        android:layout_width="match_parent"
        android:gravity="center_vertical"
        android:layout_height="101px">
        <ImageView
            android:layout_centerVertical="true"
            android:layout_marginLeft="30px"
            android:src="@mipmap/zhanghaoanquan2x"
            android:layout_width="44px"
            android:layout_height="44px" />
        <TextView
            android:layout_marginTop="1px"
            android:layout_marginLeft="100px"
            android:layout_centerVertical="true"
            android:text="帐号安全"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <ImageView
            android:layout_marginRight="30px"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@mipmap/btn_right_arrow"
            android:layout_width="15px"
            android:layout_height="24px" />
        <View
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="30px"
            android:layout_marginRight="30px"
            android:background="@color/view_background"
            android:layout_width="match_parent"
            android:layout_height="1px"/>
    </com.zhy.autolayout.AutoRelativeLayout>
    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:gravity="center_vertical"
        android:layout_height="101px">
        <ImageView
            android:layout_centerVertical="true"
            android:layout_marginLeft="30px"
            android:src="@mipmap/yinsi2x"
            android:layout_width="44px"
            android:layout_height="44px" />
        <TextView
            android:layout_marginLeft="100px"
            android:layout_centerVertical="true"
            android:text="隐私设置"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <ImageView
            android:layout_marginRight="30px"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@mipmap/btn_right_arrow"
            android:layout_width="15px"
            android:layout_height="24px" />
        <View
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="30px"
            android:layout_marginRight="30px"
            android:background="@color/view_background"
            android:layout_width="match_parent"
            android:layout_height="1px"/>
    </com.zhy.autolayout.AutoRelativeLayout>
    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:gravity="center_vertical"
        android:layout_height="101px">
        <ImageView
            android:layout_centerVertical="true"
            android:layout_marginLeft="30px"
            android:src="@mipmap/zhucexieyi2x"
            android:layout_width="44px"
            android:layout_height="44px" />
        <TextView
            android:layout_marginLeft="100px"
            android:layout_centerVertical="true"
            android:text="用户注册协议"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <ImageView
            android:layout_marginRight="30px"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@mipmap/btn_right_arrow"
            android:layout_width="15px"
            android:layout_height="24px" />
        <View
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="30px"
            android:layout_marginRight="30px"
            android:background="@color/view_background"
            android:layout_width="match_parent"
            android:layout_height="1px"/>
    </com.zhy.autolayout.AutoRelativeLayout>
    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:gravity="center_vertical"
        android:layout_height="101px">
        <ImageView
            android:layout_centerVertical="true"
            android:layout_marginLeft="30px"
            android:src="@mipmap/qingchuhuancun"
            android:layout_width="44px"
            android:layout_height="44px" />
        <TextView
            android:layout_marginLeft="100px"
            android:layout_centerVertical="true"
            android:text="清理缓存"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <ImageView
            android:layout_marginRight="30px"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@mipmap/btn_right_arrow"
            android:layout_width="15px"
            android:layout_height="24px" />
        <View
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="30px"
            android:layout_marginRight="30px"
            android:background="@color/view_background"
            android:layout_width="match_parent"
            android:layout_height="1px"/>
    </com.zhy.autolayout.AutoRelativeLayout>
    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:gravity="center_vertical"
        android:layout_height="101px">
        <ImageView
            android:layout_centerVertical="true"
            android:layout_marginLeft="30px"
            android:src="@mipmap/yijianjianyi2x"
            android:layout_width="44px"
            android:layout_height="44px" />
        <TextView
            android:layout_marginLeft="100px"
            android:layout_centerVertical="true"
            android:text="意见建议"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <ImageView
            android:layout_marginRight="30px"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@mipmap/btn_right_arrow"
            android:layout_width="15px"
            android:layout_height="24px" />
        <View
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="30px"
            android:layout_marginRight="30px"
            android:background="@color/view_background"
            android:layout_width="match_parent"
            android:layout_height="1px"/>
    </com.zhy.autolayout.AutoRelativeLayout>
</com.zhy.autolayout.AutoLinearLayout>
